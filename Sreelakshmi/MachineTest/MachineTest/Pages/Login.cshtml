@page
@model MachineTest.Pages.LoginModel
@{
}
<h2>Login</h2>
<form method="post">
    <div>
        <input asp-for="Email" placeholder="Email" />
    </div>
    <div>
        <input asp-for="Password" type="password" placeholder="Password" />
    </div>
    <button>Login</button>
    
</form>
<p>@Model.ErrorMessage</p>